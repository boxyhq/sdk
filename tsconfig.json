    "strict": true,
    "target": "es5",
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsxImportSource": "@builder.io/mitosis",
    "paths": {
      "react": ["./react/node_modules/@types/react/index.d.ts"],
      "vue": ["./vue/node_modules/vue/dist/vue.d.ts"]

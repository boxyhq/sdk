{
  "name": "@boxyhq/internal-ui",
  "description": "Shared React UI components for BoxyHQ SaaS and Admin Portal",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "directory": "react",
    "url": "https://github.com/boxyhq/ui"
  },
  "homepage": "https://github.com/boxyhq/ui",
  "bugs": {
    "url": "https://github.com/boxyhq/ui/issues?q=is%3Aopen+is%3Aissue+label%3Asdk"
  },
  "files": [
    "dist",
    "index.d.ts",
    "well-known.d.ts",
    "federated-saml.d.ts",
    "shared.d.ts"
  ],
  "types": "./index.d.ts",
  "exports": {
    "./well-known": {
      "types": "./dist/well-known/index.d.ts",
      "import": "./dist/well-known.js"
    },
    "./federated-saml": {
      "types": "./dist/federated-saml/index.d.ts",
      "import": "./dist/federated-saml.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "publish-local": "./dev/verdaccio.sh"
  },
  "dependencies": {
    "vite-tsconfig-paths": "4.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.6",
    "@types/react": "18.2.55",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "typescript": "5.3.3",
    "vite": "5.1.0",
    "@boxyhq/saml-jackson": "1.18.7"
  },
  "peerDependencies": {
    "@heroicons/react": "2.1.1",
    "next": "14.1.0",
    "next-i18next": "13.3.0",
    "react": "18.2.0",
    "react-daisyui": "5.0.0",
    "react-tagsinput": "3.20.3",
    "swr": "2.2.4",
    "formik": "2.4.5",
    "classnames": "2.5.1"
  }
